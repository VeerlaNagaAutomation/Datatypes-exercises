################
d = {
  "brand": "Ford",
  "model": "Mustang",
  "year": 1964
}
print(d.get('model')) # Mustang
##################################
d = {
"brand": "Ford",
"model": "Mustang",
"year": 1964
}

print(d.keys()) # dict_keys(['brand', 'model', 'year'])
print(d.values()) # dict_values(['Ford', 'Mustang', 1964])
print(d.items()) # dict_items([('brand', 'Ford'), ('model', 'Mustang'), ('year', 1964)])
#################################
thisdict = {
  "brand": "Ford",
  "model": "Mustang",
  "year": 1964
}
thisdict.update({"year": 2020})

print(thisdict) # {'brand': 'Ford', 'model': 'Mustang', 'year': 2020}
#############################
Adding Items:
d =	{
  "brand": "Ford",
  "model": "Mustang",
  "year": 1964
 } 
d["color"] = "red"
print(d) # {'brand': 'Ford', 'model': 'Mustang', 'year': 1964, 'color': 'red'}
###
d = {
  "brand": "Ford",
  "model": "Mustang",
  "year": 1964
}
d.update({"color": "red"})

print(d) # {'brand': 'Ford', 'model': 'Mustang', 'year': 1964, 'color': 'red'}

##############################
d =	{
  "brand": "Ford",
  "model": "Mustang",
  "year": 1964
}
d.pop("model")
print(d) # {'brand': 'Ford', 'year': 1964}
###
d =	{
  "brand": "Ford",
  "model": "Mustang",
  "year": 1964
}
d.popitem() # removes lat item
print(d) # {'brand': 'Ford', 'model': 'Mustang'}
###########################
  "brand": "Ford",
  "model": "Mustang",
  "year": 1964
}
mydict = d.copy()
print(mydict) # {'brand': 'Ford', 'model': 'Mustang', 'year': 1964}
###
d = {
  "brand": "Ford",
  "model": "Mustang",
  "year": 1964
}
mydict = dict(d)
print(mydict) # {'brand': 'Ford', 'model': 'Mustang', 'year': 1964}
