d = {
  "child1" : {
    "name" : "Emil",
    "year" : 2004
  },
  "child2" : {
    "name" : "Tobias",
    "year" : 2007
  },
  "child3" : {
    "name" : "Linus",
    "year" : 2011
  }
}

print(d['child3']['year']) # 2011

for k,v in d.items():
  print(k)
  for i,j in v.items():
  	print(i,j)
    
