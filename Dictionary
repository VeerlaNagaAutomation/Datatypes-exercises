d = {
  "brand": "Ford",
  "electric": False,
  "year": 1964,
  "colors": ["red", "white", "blue"]
}

print(d['colors'][1]) # white
#################################
thisdict = dict(name = "John", age = 36, country = "Norway")

print(thisdict) # {'name': 'John', 'age': 36, 'country': 'Norway'}
